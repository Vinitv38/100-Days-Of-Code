    "class Solution(object):\n",
    "    def isAnagram(s, t):\n",
    "        s = ''.join(sorted(s))\n",
    "        t = ''.join(sorted(t))\n",
    "        if s==t:\n",
    "            return True\n",
    "        return False\n",
    "    \n",
    "    string_1 = \"anagram\"\n",
    "    string_2 = \"nagaram\"\n",
    "    print(isAnagram( string_1, string_2))"
   ]
